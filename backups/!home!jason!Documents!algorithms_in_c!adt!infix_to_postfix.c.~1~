#include <stdio.h>
#include <string.h>
#include "stack.h"
#include "item.h"

int main(int argc, char *argv[])
{
     char *a = argv[1];
     int i, N = strlen(a);
     
     stackInit(N);
     for (i = 0; i < N; i++)
     {
          if (a[i] == ')')
               printf("%c ", stackPop());
          if ((a[i] == '+') || (a[i] == '*'))
               stackPush(a[i]);
          if ((a[i] >= '0') && (a[i] <= '9'))
               printf("%c ", a[i]);
     }
     printf ("\n");
     return 0;
}
