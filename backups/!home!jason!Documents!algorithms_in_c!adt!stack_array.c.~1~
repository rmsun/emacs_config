#include <stdlib.h>
#include "stack.h"

typedef int Item;

static Item *s;
static int N;

void stackInit(int maxN)
{
     s = malloc(maxN*sizeof(Item));
     N = 0;
}

int stackEmpty()
{
     return N == 0;
}

void stackPush(Item item)
{
     s[N++] = item;
}

Item stackPop()
{
     return s[--N];
}

