#include <stdio.h>
#include <string.h>
#include "item.h"
#include "stack.h"

int main(int argc, char *argv[])
{
     char *a = argv[1];
     int i, N = strlen(a);
     
     stackInit(N);
     for (i = 0; i < N; i++)
     {
          if (a[i] == '+')
               stackPush(stackPop() + stackPop());
          if (a[i] == '*')
               stackPush(stackPop() * stackPop());
          if ((a[i] >= '0') && (a[i] <= '9'))
               stackPush(0);
          while((a[i] >= '0') && (a[i] <= '9'))
                stackPush(10*stackPop() + (a[i++]-'0'));
   
     }
     
